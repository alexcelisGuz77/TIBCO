<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	 xmlns:Employeet="http://www.tibco.com/schemas/0.ProjectZero/Resources/Schemas/Model/Employee"
	 xmlns:Address="http://www.tibco.com/schemas/0.ProjectZero/Resources/Schemas/Model/Address"
	 targetNamespace="http://www.tibco.com/schemas/0.ProjectZero/Resources/Schemas/Model/Employee"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<xs:import namespace="http://www.tibco.com/schemas/0.ProjectZero/Resources/Schemas/Model/Address" schemaLocation="Address.xsd"/>
	<xs:element name="Employeet">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="employeeId" type="xs:string"/>
				<xs:element name="firstName" type="xs:string"/>
				<xs:element name="lastName" type="xs:string"/>
				<xs:element name="age" type="xs:int"/>
				<xs:element ref="Address:Address"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>